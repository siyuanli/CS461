       io.putInt(c.foo());
       A a = b;
       io.putInt(c.foo());
       this.printExpected("333");

       a = new A();
       io.putInt(a.foo());
       a = new B();
       io.putInt(a.foo());
       a = new C();
       io.putInt(a.foo());
       this.printExpected("543");
    }



