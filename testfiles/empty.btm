    void testScoping(){
        String s = "hi";
        this.s = "hello";
        io.putString(s);
        io.putString(this.s);
        {
            int z = 10;
            for (;true;z++){
                if (z==10){
                    {
                        int d = 6;
                        io.putInt(z);
                        io.putInt(d);
                    }
                    io.putString(this.s);
                    io.putString(s);
                }
                int i = 0;
                int j = 0;
                for (; i< 5; i++){
                    j++;
                }
                io.putInt(j);

                if (z == 15){
                    break;
                }
            }
            io.putInt(z);

        }
        int z = 20;
        io.putInt(z);
        String d = "yo";
        io.putString(d);
        this.printExpected("hihello106hellohi5555551520yo");

    }

