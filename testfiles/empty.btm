    void testFor(){
        int j = 0;
        for (;;){
            if (j == 10){
                io.putString("Breaking!");
                break;
            }
            j++;
        }
        io.putInt(j);
        this.printExpected("10");

        for (;;j++){
            if(j == 20){
                io.putString("Breaking!");
                break;
            }
        }
        io.putInt(j);
        this.printExpected("20");

        j = 0;
        int i = 0;
        for(;j<10;j++){
            i++;
        }
        io.putInt(i);
        io.putInt(j);
        this.printExpected("1010");

        i = 0;
        for(j=0; j<20; j++){
            i++;
        }
        io.putInt(i);
        io.putInt(j);
        this.printExpected("2020");

        for(j = 0; ; j++){
            if (j == 15){
                io.putString("Breaking!");
                break;
            }
        }
        io.putInt(j);
        this.printExpected("15");

        for(j = 0; j <16;){
            j++;
        }
        io.putInt(j);
        this.printExpected("16");

        for(j = 0;;){
            if (j == 5){
                io.putString("Breaking!");
                break;
            }
            j++;
        }
        io.putInt(j);
        this.printExpected("5");

        j = 0;
        i = 10;
        for (;j<10;){
            i++;
            j++;
        }
        io.putInt(j);
        io.putInt(i);
        this.printExpected("1020");
    }

