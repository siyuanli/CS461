class Main {
    TextIO io = new TextIO();
    int jo = 8;


    int testReturn(){
        return 10;
    }

    void testLocalVSField(){
        int jo = 4;
        io.putInt(jo);
        io.putInt(this.jo).putString("\n");
    }

    void testThis(){
        String yo = "yoyo";
        io.putString(yo);
        this.io.putString(yo).putString("\n");

    }

    void testMethodCallInception(){
        io.putInt(this.testReturn()).putString("\n");
    }

    int testParams(int x, int y, String z){
        io.putInt(x);
        io.putInt(y);
        io.putString(z);
        return 5;
    }


    void main() {
        int a = 2;
        int x = 3;

        int returning = this.testReturn();
        io.putInt(returning); //10

        this.testLocalVSField(); //48

        this.testThis(); //yoyoyoyo

        this.testMethodCallInception(); //10

        int d = this.testParams(a, x, "hihi"); //23hihi
        io.putInt(d).putString("\n"); //5

	}
}
